{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "env.d.ts", <% for(let i = 0; i < projectTsNames.length; i++){ %><% let projectName = projectTsNames[i] %>
    "<%= projectName %>/pages/**/*.vue", 
    "<%= projectName %>/page/**/*", 
    "<%= projectName %>/scene/**/*.vue", 
    "<%= projectName %>/scene/**/*"<%= i !== projectTsNames.length - 1 ? ',' : '' %><% } %>
  ],
  "exclude": ["node_modules", "dist"]
}